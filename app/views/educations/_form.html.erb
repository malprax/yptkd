<%= simple_form_for(@education) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nama, label: 'Nama'  %>
    <%= f.input :satuan, label: 'Satuan'  %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-success' %>&nbsp;&nbsp;<%= link_to 'Kembali', educations_path, class:'btn btn-default' %>
  </div>
<% end %>
