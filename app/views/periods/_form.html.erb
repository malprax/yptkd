<%= simple_form_for(@period) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :bulan, label: 'Bulan', collection: ['Januari','Februari','Maret','April','Mei','Juni','Juli','Agustus','September','Nopember','Desember'] %>
    <%= f.input :tahun, label: 'Tahun' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-default' %>&nbsp;&nbsp;<%= link_to 'Kembali', periods_path, class: 'btn btn-success' %>
  </div>
<% end %>
