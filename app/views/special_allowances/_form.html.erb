<%= simple_form_for(@special_allowance) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nama %>
    <%= f.input :satuan %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-default' %>&nbsp;&nbsp;<%= link_to 'Kembali', special_allowances_path, class: 'btn btn-success' %>
  </div>
<% end %>
