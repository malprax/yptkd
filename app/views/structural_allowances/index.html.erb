<p id="notice"><%= notice %></p>

<h1>Listing Structural Allowances</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Satuan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @structural_allowances.each do |structural_allowance| %>
      <tr>
        <td><%= structural_allowance.nama %></td>
        <td><%= structural_allowance.satuan %></td>
        <td><%= link_to 'Show', structural_allowance %></td>
        <td><%= link_to 'Edit', edit_structural_allowance_path(structural_allowance) %></td>
        <td><%= link_to 'Destroy', structural_allowance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Structural allowance', new_structural_allowance_path %>
