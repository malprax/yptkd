<div class='container'>
  <div class='row'>
    <h3 class='lead'>Rincian Potongan Dan Satuan Biayanya</h3>
    <table class='table table-hover table-striped'>
      <thead>
        <tr>
          <th>Nama</th>
          <th>Satuan</th>
        </tr>
      </thead>

      <tbody>
        <% @taxes.each do |tax| %>
          <tr>
            <td><%= tax.nama %></td>
            <td><%= tax.satuan %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

  </div>
</div>
